            </tr>
            <tr>
                <th>RappoteurNom</th>
                <td>{{ evenement.rappoteurNom }}</td>
            </tr>
            <tr>
                <th>RapporteurPrenom</th>
                <td>{{ evenement.rapporteurPrenom }}</td>
            </tr>
            <tr>
                <th>Lien</th>
                <td>{{ evenement.lien }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('evenement_index') }}">back to list</a>

    <a href="{{ path('evenement_edit', {'id': evenement.id}) }}">edit</a>

    {{ include('evenement/_delete_form.html.twig') }}
